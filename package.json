    "@graphql-tools/utils": "^8.5.5",
    "@types/mongodb": "^4.0.7",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "graphql-import-node": "0.0.4",
